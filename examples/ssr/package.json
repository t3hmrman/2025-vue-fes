{
    "name": "ssr",
    "version": "0.1.0",
    "description": "Example of Vue's built in SSR features with WebAssembly",
    "type": "module",
    "scripts": {
        "fetch:wit": "wkg wit fetch",
        "build:ts": "rolldown -c component.rolldown.config.ts",
        "build:client": "vite build -c client.vite.config.ts",
        "build:component": "pnpm build:ts && jco componentize -w wit -o dist/wasm/component.wasm dist/wasm/component.js",
        "build": "pnpm build:client && pnpm build:component",
        "serve": "wasmtime serve -Scli dist/wasm/component.wasm",
        "serve:prebuilt": "wasmtime serve -Scli prebuilt.wasm"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.2.3",
        "@vueuse/core": "^14.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-vue-next": "^0.546.0",
        "reka-ui": "^2.6.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.16",
        "tw-animate-css": "^1.4.0",
        "vue": "catalog:"
    },
    "devDependencies": {
        "@bytecodealliance/jco": "catalog:",
        "@tailwindcss/vite": "^4.1.16",
        "rolldown": "catalog:",
        "rolldown-vite": "catalog:",
        "rollup-plugin-string-import": "catalog:",
        "shadcn-vue": "^2.3.2",
        "vite": "npm:rolldown-vite@^7.1.19"
    }
}
